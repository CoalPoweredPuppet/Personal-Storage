<script type="text/worker">

    //Renown   
on("change:renown_type sheet:opened", function() {
    getAttrs(['renown_type'], function(values) {
            //Ajaba
            if (values.renown_type == 1 ) {
                setAttrs({
                    renown1_name: 'Ferocity',
                    renown2_name: 'Cunning',
                    renown3_name: 'Obligation'
                })
            }
            //Ananasi
            else if (values.renown_type == 2 ) {
                setAttrs({
                    renown1_name: 'Cunning',
                    renown2_name: 'Obedience',
                    renown3_name: 'Wisdom'
                })
            }
            //Bastet
            else if (values.renown_type == 3 ) {
                setAttrs({
                    renown1_name: 'Ferocity',
                    renown2_name: 'Cunning',
                    renown3_name: 'Honor'
                })
            }
            //Corax
            else if (values.renown_type == 4 ) {
                setAttrs({
                    renown1_name: 'Glory',
                    renown2_name: 'Honor',
                    renown3_name: 'Wisdom'
                })
            }
            //Garou
            else if (values.renown_type == 5 ) {
                setAttrs({
                    renown1_name: 'Glory',
                    renown2_name: 'Honor',
                    renown3_name: 'Wisdom'
                })
            }
            //Gurahl
            else if (values.renown_type == 6 ) {
                setAttrs({
                    renown1_name: 'Honor',
                    renown2_name: 'Succor',
                    renown3_name: 'Wisdom'
                })
            }
            //Humanity
            else if (values.renown_type == 7 ) {
                setAttrs({
                    renown1_name: '',
                    renown2_name: '',
                    renown3_name: ''
                })
            }
            //Kitsune
            else if (values.renown_type == 8 ) {
                setAttrs({
                    renown1_name: 'Chie (Cunning)',
                    renown2_name: 'Toru (Honor)',
                    renown3_name: 'Kagayaki (Glory)'
                })
            }
            //Mokol
            else if (values.renown_type ==  9 ) {
                setAttrs({
                    renown1_name: 'Glory',
                    renown2_name: 'Honor',
                    renown3_name: 'Wisdom'
                })
            }
            //Nagah
            else if (values.renown_type == 10 ) {
                setAttrs({
                    renown1_name: 'n/a',
                    renown2_name: 'n/a',
                    renown3_name: 'n/a'
                })
            }
            //Nuwisha
            else if (values.renown_type == 11 ) {
                setAttrs({
                    renown1_name: 'Cunning',
                    renown2_name: 'Glory',
                    renown3_name: 'Humor'
                })
            }
            //Ratkin
            else if (values.renown_type == 12 ) {
                setAttrs({
                    renown1_name: 'Cunning',
                    renown2_name: 'Infamy',
                    renown3_name: 'Obligation'
                })
            }
            //Rokea
            else if (values.renown_type == 13 ) {
                setAttrs({
                    renown1_name: 'Valor',
                    renown2_name: 'Harmony',
                    renown3_name: 'Innovation'
                })
            }
        });
    });
