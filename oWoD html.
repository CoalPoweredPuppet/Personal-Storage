on("change:splat_type sheet:opened", function() {
    getAttrs(["splat_type"], function(values) {

        if (values.splat_type == 'Human') {
            setAttrs({ 
                splat_select: 1,
                splat_select1: 1,
                splat_select2: 1,
                splat_select3: 1,
                splat_talents: 1,
                splat_skills: 1,
                splat_knowledges: 1,
                splat_passions: 1
            })
        }
        else if (values.splat_type == 'Vampire') {
            setAttrs({ 
                splat_select: 2,
                splat_select1: 2,
                splat_select2: 2,
                splat_select3: 2,
                splat_talents: 2,
                splat_skills: 2,
                splat_knowledges: 2,
                splat_passions: 2
            })
        }
        else if (values.splat_type == 'Werewolf') {
            setAttrs({ 
                splat_select: 3,
                splat_select1: 3,
                splat_select2: 3,
                splat_select3: 3,
                splat_talents: 3,
                splat_skills: 3,
                splat_knowledges: 3,
                splat_passions: 3
            })
        }
        else if (values.splat_type == 'Mage') {
            setAttrs({ 
                splat_select: 4,
                splat_select1: 4,
                splat_select2: 4,
                splat_select3: 4,
                splat_talents: 4,
                splat_skills: 4,
                splat_knowledges: 4,
                splat_passions: 4
            })
        }
        else if (values.splat_type == 'Wraith') {
            setAttrs({ 
                splat_select: 5,
                splat_select1: 5,
                splat_select2: 5,
                splat_select3: 5,
                splat_talents: 5,
                splat_skills: 5,
                splat_knowledges: 5,
                splat_passions: 5
            })
        }
        else if (values.splat_type == 'Changeling') {
            setAttrs({ 
                splat_select: 6,
                splat_select1: 6,
                splat_select2: 6,
                splat_select3: 6,
                splat_talents: 6,
                splat_skills: 6,
                splat_knowledges: 6,
                splat_passions: 6
            })
        }
        else if (values.splat_type == 'All') {
            setAttrs({ 
                splat_select: 99,
                splat_select1: 99,
                splat_select2: 99,
                splat_select3: 99,
                splat_talents: 99,
                splat_skills: 99,
                splat_knowledges: 99,
                splat_passions: 99
            })
        }
    });
});
        
on("change:skill_select sheet:opened", function() {
    getAttrs(["skill_select"], function(values) {

        if (values.skill_select == '0') {
            setAttrs({ 
                skill: 'No Skill'
            })
        }
        else if (values.skill_select == '@{alertness}') {
            setAttrs({ 
                skill: 'Alertness'
            })
        }
        else if (values.skill_select == '@{athletics}') {
            setAttrs({ 
                skill: 'Athletics'
            })
        }
        else if (values.skill_select == '@{brawl}') {
