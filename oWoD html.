            </tr>
            {{/damage}}
            
            {{#attack}} 
            <tr>
                <th>{{name}} attack with {{attack}}</th>
            </tr>
            {{/attack}}
            {{#pool}} 
            <tr>
                <th>{{name}} rolls {{pool}} {{#spec}}<div style="color:blue;">{{spec}}</div>{{/spec}}</th>
            </tr>
            {{/pool}}
            <tr>
                <td class="sheet-attr">
                    {{attr}}
                    {{#skill}}<br>
                     + {{skill}}
                    {{/skill}}
                    {{#mod}}<br>
                     + {{mod}}
                    {{/mod}} 
                    {{#wound}}<br>
                     + {{wound}}
                    {{/wound}}
                </td>
            </tr>
            {{#initiative}}
            <tr>
                <td class="sheet-result">
                    Initiative  {{initiative}}
                </td>        
            </tr>
            {{/initiative}}
            {{#result}}
            <tr>
                <td class="sheet-result">
                    {{result}} Successes
                    {{#type}}
                        <br><div style="color:red;">{{type}}</div>
                    {{/type}}
                </td>        
            </tr>
            {{/result}}
        </table>
    </rolltemplate>
    
