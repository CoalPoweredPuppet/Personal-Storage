                skill: 'Occult'
            })
        }
        else if (values.skill_select == '@{rituals}') {
            setAttrs({ 
                skill: 'Rituals'
            })
        }
        else if (values.skill_select == '@{science}') {
            setAttrs({ 
                skill: 'Science'
            })
        }
        else if (values.skill_select == '@{technology}') {
            setAttrs({ 
                skill: 'Technology'
            })
        }
    });
});
on("change:attribute_select sheet:opened", function() {
    getAttrs(["attribute_select"], function(values) {

        if (values.attribute_select == '0') {
            setAttrs({ 
                attribute: 'No Attribute'
            })
        }
        else if (values.attribute_select == '@{strength}') {
            setAttrs({ 
                attribute: 'Strength'
            })
        }
        else if (values.attribute_select == '@{charisma}') {
            setAttrs({ 
                attribute: 'Charisma'
            })
        }
        else if (values.attribute_select == '@{perception}') {
            setAttrs({ 
                attribute: 'Perception'
            })
        }
        else if (values.attribute_select == '@{dexterity}') {
            setAttrs({ 
                attribute: 'Dexterity'
            })
        }
        else if (values.attribute_select == '@{manipulation}') {
            setAttrs({ 
                attribute: 'Manipulation'
            })
        }
        else if (values.attribute_select == '@{intelligence}') {
            setAttrs({ 
                attribute: 'Intelligence'
            })
        }
        else if (values.attribute_select == '@{stamina}') {
            setAttrs({ 
                attribute: 'Stamina'
            })
        }
        else if (values.attribute_select == '@{appearance}') {
            setAttrs({ 
                attribute: 'Appearance'
            })
        }
        else if (values.attribute_select == '@{wits}') {
            setAttrs({ 
                attribute: 'Wits'
            })
        }
    });
});
        
on("change:current_form  change:strength_base change:charisma_base change:perception_base change:dexterity_base change:manipulation_base change:intelligence_base change:stamina_base change:appearance_base change:wits_base change:strength_man change:charisma_man change:perception_man change:dexterity_man change:manipulation_man change:intelligence_man change:stamina_man change:appearance_man change:wits_man change:strength_near_man change:charisma_near_man change:perception_near_man change:dexterity_near_man change:manipulation_near_man change:intelligence_near_man change:stamina_near_man change:appearance_near_man change:wits_near_man change:strength_hybrid change:charisma_hybrid change:perception_hybrid change:dexterity_hybrid change:manipulation_hybrid change:intelligence_hybrid change:stamina_hybrid change:appearance_hybrid change:wits_hybrid change:strength_near_beast change:charisma_near_beast change:perception_near_beast change:dexterity_near_beast change:manipulation_near_beast change:intelligence_near_beast change:stamina_near_beast change:appearance_near_beast change:wits_near_beast change:strength_near_man change:charisma_near_man change:perception_near_man change:dexterity_near_man change:manipulation_near_man change:intelligence_near_man change:stamina_near_man change:appearance_near_man change:wits_near_man change:strength_beast change:charisma_beast change:perception_beast change:dexterity_beast change:manipulation_beast change:intelligence_beast change:stamina_beast change:appearance_beast change:wits_beast sheet:opened", function() {
    getAttrs(['current_form', 'strength_base', 'charisma_base', 'perception_base', 'dexterity_base', 'manipulation_base', 'intelligence_base', 'stamina_base', 'appearance_base', 'wits_base', 'strength_man', 'charisma_man', 'perception_man', 'dexterity_man', 'manipulation_man', 'intelligence_man', 'stamina_man', 'appearance_man', 'wits_man', 'strength_near_man', 'charisma_near_man', 'perception_near_man', 'dexterity_near_man', 'manipulation_near_man', 'intelligence_near_man', 'stamina_near_man', 'appearance_near_man', 'wits_near_man', 'strength_hybrid', 'charisma_hybrid', 'perception_hybrid', 'dexterity_hybrid', 'manipulation_hybrid', 'intelligence_hybrid', 'stamina_hybrid', 'appearance_hybrid', 'wits_hybrid', 'strength_near_beast', 'charisma_near_beast', 'perception_near_beast', 'dexterity_near_beast', 'manipulation_near_beast', 'intelligence_near_beast', 'stamina_near_beast', 'appearance_near_beast', 'wits_near_beast', 'strength_near_man', 'charisma_near_man', 'perception_near_man', 'dexterity_near_man', 'manipulation_near_man', 'intelligence_near_man', 'stamina_near_man', 'appearance_near_man', 'wits_near_man', 'strength_beast', 'charisma_beast', 'perception_beast', 'dexterity_beast', 'manipulation_beast', 'intelligence_beast', 'stamina_beast', 'appearance_beast', 'wits_beast'], function(values) {
            if (values.current_form == 1 ) {
                setAttrs({
                    strength: +values.strength_base + +values.strength_man,
                    charisma: +values.charisma_base + +values.charisma_man,
                    perception: +values.perception_base + +values.perception_man,
                    dexterity: +values.dexterity_base + +values.dexterity_man,
                    manipulation: +values.manipulation_base + +values.manipulation_man,
                    intelligence: +values.intelligence_base + +values.intelligence_man,
                    stamina: +values.stamina_base + +values.stamina_man,
                    appearance: +values.appearance_base + +values.appearance_man,
                    wits: +values.wits_base + +values.wits_man
                    
        		})
            }
          	else if (values.current_form == 2 ) {
                setAttrs({
                    strength: +values.strength_base + +values.strength_near_man,
                    charisma: +values.charisma_base + +values.charisma_near_man,
                    perception: +values.perception_base + +values.perception_near_man,
                    dexterity: +values.dexterity_base + +values.dexterity_near_man,
                    manipulation: +values.manipulation_base + +values.manipulation_near_man,
                    intelligence: +values.intelligence_base + +values.intelligence_near_man,
                    stamina: +values.stamina_base + +values.stamina_near_man,
                    appearance: +values.appearance_base + +values.appearance_near_man,
                    wits: +values.wits_base + +values.wits_near_man
                    
                    
        		})
            }
          	else if (values.current_form == 3 ) {
                setAttrs({
                    strength: +values.strength_base + +values.strength_hybrid,
                    charisma: +values.charisma_base + +values.charisma_hybrid,
                    perception: +values.perception_base + +values.perception_hybrid,
                    dexterity: +values.dexterity_base + +values.dexterity_hybrid,
                    manipulation: +values.manipulation_base + +values.manipulation_hybrid,
                    intelligence: +values.intelligence_base + +values.intelligence_hybrid,
                    stamina: +values.stamina_base + +values.stamina_hybrid,
                    appearance: +values.appearance_base + +values.appearance_hybrid,
                    wits: +values.wits_base + +values.wits_hybrid
                    
                    
        		})
            }
          	else if (values.current_form == 4 ) {
                setAttrs({
                    strength: +values.strength_base + +values.strength_near_beast,
                    charisma: +values.charisma_base + +values.charisma_near_beast,
                    perception: +values.perception_base + +values.perception_near_beast,
                    dexterity: +values.dexterity_base + +values.dexterity_near_beast,
                    manipulation: +values.manipulation_base + +values.manipulation_near_beast,
                    intelligence: +values.intelligence_base + +values.intelligence_near_beast,
                    stamina: +values.stamina_base + +values.stamina_near_beast,
                    appearance: +values.appearance_base + +values.appearance_near_beast,
                    wits: +values.wits_base + +values.wits_near_beast
                    
        		})
            }
          	else if (values.current_form == 5 ) {
                setAttrs({
                    strength: +values.strength_base + +values.strength_beast,
                    charisma: +values.charisma_base + +values.charisma_beast,
                    perception: +values.perception_base + +values.perception_beast,
                    dexterity: +values.dexterity_base + +values.dexterity_beast,
                    manipulation: +values.manipulation_base + +values.manipulation_beast,
                    intelligence: +values.intelligence_base + +values.intelligence_beast,
                    stamina: +values.stamina_base + +values.stamina_beast,
                    appearance: +values.appearance_base + +values.appearance_beast,
                    wits: +values.wits_base + +values.wits_beast
					
			    })
            }
        });
    });
   

on("change:healthbox_1 change:healthbox_2 change:healthbox_3 change:healthbox_4 change:healthbox_5 change:healthbox_6 change:healthbox_7 change:healthbox_8 sheet:opened", function() {
    getAttrs(['healthbox_1', 'healthbox_2', 'healthbox_3', 'healthbox_4', 'healthbox_5', 'healthbox_6', 'healthbox_7', 'healthbox_8'], function(values) {
        setAttrs({ 
            injury: ((Math.floor(Math.max.apply(null, Object.values(values)))) * -1) 
        });
    });
});
</script>
